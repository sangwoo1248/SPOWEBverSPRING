<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="AdminMapper">

<insert id="adminAdd" parameterType="AdminDTO">
	insert into admin(admin_id,admin_passwd,admin_username)
	values(#{admin_id},#{admin_passwd},#{admin_username})
</insert>
	
</mapper>